<%- include("partials/header") -%>

    <h1>
        Compose
    </h1>
    <form action="/compose" method="post">
        <div class="mb-3">
            <label for="exampleFormControlInput1" class="form-label">Title</label>
            <input type="text" name="composeTitle" class="form-control" id="exampleFormControlInput1" placeholder="">
        </div>
        <div class="mb-3">
            <label for="exampleFormControlTextarea1" class="form-label">Post</label>
            <textarea name="composeItem" class="form-control" id="exampleFormControlTextarea1" rows="3"></textarea>
        </div>
        <button type="submit" class="btn btn-lg btn-primary"  id="composeSub">Submit</button>

    </form>

    <%- include("partials/footer") -%>